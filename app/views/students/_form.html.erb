<%= form_for @student do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
  <fieldset>
  <legend>Create/Edit Student</legend>
    <div class="control-group">
    <%= f.label :number, class: "control-label" %>
    <div class="controls">
    <%= f.text_field :number %>
    </div>
    </div>
    <div class="control-group">
    <%= f.label :first, class: "control-label" %>
    <div class="controls">
    <%= f.text_field :first %>
    </div>
    </div>
    <div class="control-group">
    <%= f.label :last, class: "control-label" %>
    <div class="controls">
    <%= f.text_field :last %>
    </div>
    </div>
    <div class="control-group">
    <%= f.label :email, class: "control-label" %>
    <div class="controls">
    <%= f.text_field :email %>
    </div>
    </div>
    <div class="form-actions">
    <%= f.submit class: "btn btn-primary btn-xs"%>
    </div>
  </fieldset>

<% end %>
